<div class="row">
   <div class="col s12 center-align">
     <div class="card blue-grey darken-1">
       <div class="card-content white-text">
         <span class="card-title">
           <h4>Population</h4>
           <p><%= @total_robots %></p>
         </span>
         <h5>Average Age</h5>
         <p><%= @average_age %></p>
       </div>
     </div>
   </div>
 </div>
 <div class='row'>
  <% @all_info.each do |dataset| %>
    <div class="col s3">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"></span>
          <table>
            <th><h5><%= dataset[1] %></h5></th>
            <% dataset[0].each do |key, value| %>
            <tr>
              <td><%= key %></td>
              <td><%= value %></td>
            </tr>
            <% end %>
          </table>
        </div>
      </div>
    </div>
  <% end %>
</div>
